{
	"remove": [
		"devDependencies",
		"dependencies",
		"scripts",
		"prettier",
		"config"
	],
	"replace": {
		"main": "dist/index.cjs",
		"module": "dist/index.js",
		"types": "dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js",
				"require": "./dist/index.cjs"
			},
			"./typebox": {
				"types": "./dist/typebox.d.ts",
				"import": "./dist/typebox.js",
				"require": "./dist/typebox.cjs"
			},
			"./package.json": "./package.json"
		}
	}
}
