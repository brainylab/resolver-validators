{
  "remove": ["devDependencies", "dependencies", "scripts"],
  "replace": {
    "type": "module",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./typebox": {
        "types": "./dist/resolvers/typebox.d.ts",
        "import": "./dist/resolvers/typebox.js",
        "default": "./dist/resolvers/typebox.js"
      },
      "./zod": {
        "types": "./dist/resolvers/zod.d.ts",
        "import": "./dist/resolvers/zod.js",
        "default": "./dist/resolvers/zod.js"
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        "typebox": ["./dist/resolvers/typebox.d.ts"],
        "zod": ["./dist/resolvers/zod.d.ts"],
        ".": ["./dist/index.d.ts"]
      }
    }
  }
}
