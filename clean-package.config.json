{
	"remove": [
		"devDependencies",
		"dependencies",
		"scripts",
		"prettier",
		"config"
	],
	"replace": {
		"main": "./index.cjs",
		"module": "./index.js",
		"types": "./index.d.ts",
		"exports": {
			".": {
				"types": "./index.d.ts",
				"import": "./index.js",
				"require": "./index.cjs"
			},
			"./typebox": {
				"types": "./typebox/index.d.ts",
				"import": "./typebox/index.js",
				"require": "./typebox/index.cjs"
			},
			"./package.json": "./package.json"
		}
	}
}
